  "bugs": {
    "url": "https://github.com/Tohur-Xbl/Megatron/issues"
  },
  "homepage": "https://github.com/Tohur-Xbl/Megatron",
  "dependencies": {
    "better-sqlite-pool": "^0.2.2",
    "better-sqlite3": "^5.4.0",
    "discord.js": "github:discordjs/discord.js",
    "enmap": "^4.8.4",
    "glob": "^7.1.3",
    "logger": "github:zfbx/logger",
    "mathjs": "^5.9.0",
    "node-cron": "^2.0.3",
    "node-fetch": "^2.3.0",
    "superagent": "^5.0.5",
    "time": "^0.12.0"
  },
  "optionalDependencies": {
