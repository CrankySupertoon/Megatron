{
  "name": "megatron",
  "version": "3.3.0",
  "description": "my new discord.js bot",
  "private": true,
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "setup": "node setup.js",
    "startpm2": "pm2 start app.js --name=\"Megatron\"",
    "restart": "pm2 restart Megatron",
    "stoppm2": "pm2 stop Megatron",
    "start": "node app.js",
    "log": "pm2 logs",
    "logs": "pm2 logs",
    "startlog": "pm2 start app.js --name=\"Megatron\" && pm2 logs",
    "watch": "pm2 start app.json --name=\"Megatron\" --watch && pm2 logs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tohur-Xbl/Megatron.git"
